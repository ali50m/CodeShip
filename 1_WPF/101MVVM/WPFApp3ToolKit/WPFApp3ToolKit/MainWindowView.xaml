<Window x:Class="WPFApp3ToolKit.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFApp3ToolKit"
        mc:Ignorable="d"
        Title="MainWindow"
        Height="302.046"
        Width="559.083">
    <Window.DataContext>
        <local:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Button Content="{Binding ContentA}" x:Name="ButtonA"
                Command="{Binding CommandA}"
                HorizontalAlignment="Left"
                Margin="219,118,0,0"
                VerticalAlignment="Top"
                Width="75" />
        <Button Content="{Binding ContentB}" x:Name="ButtonB"
                Command="{Binding CommandB}"
                HorizontalAlignment="Left"
                Margin="219,176,0,0"
                VerticalAlignment="Top"
                Width="75" />

    </Grid>
</Window>
