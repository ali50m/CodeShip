<Window x:Class="ChildElementApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ChildElementApp"
        xmlns:net="clr-namespace:System;assembly=mscorlib"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <Rectangle Height="110" Fill="Orange">
            <Rectangle.Width>140</Rectangle.Width>
        </Rectangle>
        <Ellipse Height="90" Width="180" Fill="LightBlue">
        </Ellipse>
        <Button>
            <Ellipse Height="90" Width="180" Fill="LightBlue">
            </Ellipse>
        </Button>
        <Button>
            Hello // content here is a string
        </Button>
        <ListBox>
            <ListBoxItem Content="Barcelona"/>
            <ListBoxItem Content="Singapore"/>
        </ListBox>
        <Button>
            <net:DateTime>3/14/2019</net:DateTime>
        </Button>
    </StackPanel>
</Window>
